{
  "content_scripts": [
    {
      "js": [
        "data/lib/jquery.js", 
        "data/injector.js", 
        "data/injections/facebook.js"
      ], 
      "matches": [
        "*://facebook.com/*"
      ]
    }, 
    {
      "js": [
        "data/lib/jquery.js", 
        "data/injector.js", 
        "data/injections/github.js"
      ], 
      "matches": [
        "*://github.com/*"
      ]
    }
  ], 
  "description": "Use hipchat emoticons on other websites. Some sites may require this plugin in order to see the emoticons.", 
  "manifest_version": 2, 
  "name": "Hipchat-Emoticons-for-all", 
  "version": "0.0.0"
}